{
  "version": 2,
  "builds": [
    {
      "src": "web/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/.well-known/(.*)",
      "dest": "/.well-known/$1",
      "headers": {
        "Content-Type": "application/octet-stream"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
